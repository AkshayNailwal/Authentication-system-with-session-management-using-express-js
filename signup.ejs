          <h1 class="text-light bg-success rounded">Signup</h1></br>
          <form class = "form-signin" method="post" action="/signup">
  	<div class="form-label-group">
    		<label for="firstname" class="text-light">First Name</label>
    		<input type="text" class="form-control" id="firstname" name="firstname" value=""aria-describedby="emailHelp" placeholder="firstname">
    		<small id="Help" style="color:red"></small>
  	</div></br>
  	<div class="form-label-group">
    		<label for="lastname" class="text-light">Last Name</label>
    		<input type="text" class="form-control " id="lastname" name="lastname" value="" aria-describedby="emailHelp" placeholder="lastname">
    		<small id="Help" style="color:red"></small>
  	</div></br>
  	<div class="form-label-group">
    		<label for="exampleInputEmail1" class="text-light">Email address</label>
    		<input type="email" class="form-control" id="exampleInputEmail1" name="email" value="" aria-describedby="emailHelp" placeholder="Enter email">
    		<small id="Help" style="color:red"></small>
  	</div></br>
  	<div class="form-label-group">
    		<label for="username" class="text-light">Username</label>
    		<input type="text" class="form-control" id="username" name="username" value="" aria-describedby="emailHelp" placeholder="user_name">
    		<small id="Help" style="color:red"></small>
  	</div></br>
  	<div class="form-label-group">
    		<label for="exampleInputPassword1" class="text-light">Password</label>
    		<input type="password" class="form-control" id="exampleInputPassword1" name="password" value="" placeholder="Password">
    		<small id="Help">* Must be 8-20 characters long</small>
  	</div></br>
  	<div class="custom-control custom-checkbox mb-3 form-group form-check">
    		<input type="checkbox" class="custom-control-input form-check-input" id="exampleCheck1">
    		<label class="custom-control-label form-check-label" for="exampleCheck1">Check me out</label>
  	</div>
  	<div class="mx-auto align-middle">
  	<button type="submit" class="btn btn-primary">Submit</button>
  	</div>
      </form>
